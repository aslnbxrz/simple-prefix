{
  "name": "aslnbxrz/simple-prefix",
  "description": "Tiny, fast Eloquent prefix generator (trait + optional interface) with zero-hassle defaults.",
  "type": "library",
  "license": "MIT",
  "version": "1.0.3",
  "keywords": [
    "laravel",
    "eloquent",
    "prefix",
    "trait",
    "model",
    "accessor"
  ],
  "autoload": {
    "psr-4": {
      "Aslnbxrz\\SimplePrefix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aslnbxrz\\SimplePrefix\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "aslnbxrz",
      "email": "bexruz.aslonov1@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.0",
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aslnbxrz\\SimplePrefix\\SimplePrefixServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --coverage-html coverage",
    "bump-patch": "./scripts/bump-version.sh patch",
    "bump-minor": "./scripts/bump-version.sh minor",
    "bump-major": "./scripts/bump-version.sh major",
    "release": "./scripts/bump-version.sh patch"
  }
}
